<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Departement" table="departement">
    <id name="id" type="integer" column="Id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="num" type="string" column="num" length="11" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dept" type="string" column="dept" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cheflieu" type="string" column="ChefLieu" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idRegion" type="integer" column="Id_Region" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="departementlimitrophe" type="string" column="DepartementLimitrophe" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
