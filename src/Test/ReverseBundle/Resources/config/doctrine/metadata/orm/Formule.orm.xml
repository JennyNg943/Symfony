<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Formule" table="formule">
    <id name="idFormule" type="integer" column="Id_Formule">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="intituleformule" type="string" column="IntituleFormule" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="detailformule" type="text" column="DetailFormule" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nbjoursenligne" type="integer" column="NbJoursEnLigne" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="nomchampsrecruteurformule" type="string" column="NomChampsRecruteurFormule" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nomchampshistorique" type="string" column="NomChampsHistorique" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
