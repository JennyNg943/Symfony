<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Contactadmin" table="contactadmin">
    <id name="idContactadmin" type="integer" column="Id_ContactAdmin">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="Nom" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prenom" type="string" column="Prenom" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="societe" type="string" column="Societe" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tel" type="string" column="Tel" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mail" type="string" column="Mail" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="objet" type="string" column="Objet" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="message" type="text" column="Message" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idEmetteur" type="integer" column="Id_Emetteur" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idTypeemetteur" type="integer" column="Id_TypeEmetteur" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="datecreation" type="date" column="DateCreation" nullable="false"/>
    <field name="idSiteemploi" type="integer" column="Id_SiteEmploi" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
