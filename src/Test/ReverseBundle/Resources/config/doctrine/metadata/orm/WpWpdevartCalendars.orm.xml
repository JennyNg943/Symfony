<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="WpWpdevartCalendars" table="wp_wpdevart_calendars">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userId" type="integer" column="user_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="title" type="string" column="title" length="128" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="hoursEnabled" type="string" column="hours_enabled" length="3" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="hoursIntervalEnabled" type="string" column="hours_interval_enabled" length="3" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="themeId" type="integer" column="theme_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="formId" type="integer" column="form_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="extraId" type="integer" column="extra_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
