<?php

namespace OC\PlatformBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Salonsiteemploi
 *
 * @ORM\Table(name="salonsiteemploi")
 * @ORM\Entity
 */
class Salonsiteemploi
{
    /**
     * @var integer
     *
     * @ORM\Column(name="Id_SalonSiteEmploi", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $idSalonsiteemploi;

    /**
     * @var integer
     *
     * @ORM\Column(name="Id_Salon", type="integer", nullable=false)
     */
    private $idSalon;

    /**
     * @var integer
     *
     * @ORM\Column(name="Id_SiteEmploi", type="integer", nullable=false)
     */
    private $idSiteemploi;



    /**
     * Get idSalonsiteemploi
     *
     * @return integer
     */
    public function getIdSalonsiteemploi()
    {
        return $this->idSalonsiteemploi;
    }

    /**
     * Set idSalon
     *
     * @param integer $idSalon
     *
     * @return Salonsiteemploi
     */
    public function setIdSalon($idSalon)
    {
        $this->idSalon = $idSalon;

        return $this;
    }

    /**
     * Get idSalon
     *
     * @return integer
     */
    public function getIdSalon()
    {
        return $this->idSalon;
    }

    /**
     * Set idSiteemploi
     *
     * @param integer $idSiteemploi
     *
     * @return Salonsiteemploi
     */
    public function setIdSiteemploi($idSiteemploi)
    {
        $this->idSiteemploi = $idSiteemploi;

        return $this;
    }

    /**
     * Get idSiteemploi
     *
     * @return integer
     */
    public function getIdSiteemploi()
    {
        return $this->idSiteemploi;
    }
}
